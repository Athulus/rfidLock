#!/usr/bin/python3

import rfidLock
import json
import mysql.connector

def main():
  db = None
  try:
    with open("db_config.json") as db_config:
      db = mysql.connector.connect(**json.load(db_config))
    member_db = MemberDatabase(db)
    member_db.create()
    db.commit()
    db.close()
  except FileNotFoundError:
    print("db_config.json must exist to install the database")

if __name__ == '__main__':
  main()

